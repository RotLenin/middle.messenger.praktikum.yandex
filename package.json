{
  "name": "yapracticum",
  "version": "1.0.0",
  "description": "yapracticum",
  "scripts": {
    "styleLint": "npx stylelint 'src/**/*.css'",
    "styleLintFix": "npx stylelint 'src/**/*.css' --fix",
    "eslint": "eslint 'src/**/*.ts'",
    "eslintFix": "eslint --fix 'src/**/*.ts'",
    "test": "mocha",
    "dev": "NODE_ENV=development webpack-dev-server --open",
    "start": "NODE_ENV=production webpack && node index.ts",
    "startAfterBuild": "node index.ts",
    "build": "NODE_ENV=production webpack"
  },
  "keywords": [],
  "author": "Gavrik Alexandr",
  "license": "MIT",
  "dependencies": {
    "express": "4.17.3",
    "pug": "3.0.2"
  },
  "devDependencies": {
    "@types/chai": "4.3.0",
    "@types/mocha": "9.1.0",
    "@types/node": "17.0.21",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "autoprefixer": "10.4.2",
    "chai": "4.3.6",
    "css-loader": "6.7.1",
    "eslint": "8.10.0",
    "eslint-config-google": "^0.14.0",
    "file-loader": "6.2.0",
    "html-loader": "3.1.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "^7.0.4",
    "jsdom": "19.0.0",
    "jsdom-global": "3.0.2",
    "mocha": "9.2.1",
    "postcss": "8.4.8",
    "postcss-custom-properties": "12.1.4",
    "postcss-import": "14.0.2",
    "postcss-loader": "6.2.1",
    "postcss-modules": "4.3.1",
    "postcss-url": "10.1.3",
    "pug-loader": "2.4.0",
    "sharp": "0.29.3",
    "style-loader": "3.3.1",
    "stylelint": "14.5.3",
    "stylelint-config-standard": "24.0.0",
    "ts-loader": "^9.2.7",
    "ts-node": "10.6.0",
    "typescript": "^4.6.2",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          "> 1%",
          "last 4 versions",
          "ie >= 9"
        ]
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit" : "yarn test"
    }
  }
}
