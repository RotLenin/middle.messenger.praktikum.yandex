{
  "name": "yapracticum",
  "version": "1.0.0",
  "description": "yapracticum",
  "scripts": {
    "styleLint": "npx stylelint 'src/**/*.css'",
    "styleLintFix": "npx stylelint 'src/**/*.css' --fix",
    "eslint": "eslint 'src/**/*.ts'",
    "eslintFix": "eslint --fix 'src/**/*.ts'",
    "test": "mocha",
    "start": "parcel build src/index.html && node index.ts",
    "startAfterBuild": "node index.ts",
    "dev": "parcel src/index.html --no-cache",
    "build": "parcel build src/index.html"
  },
  "keywords": [],
  "author": "Gavrik Alexandr",
  "license": "MIT",
  "devDependencies": {
    "@parcel/config-default": "^2.0.1",
    "@parcel/transformer-pug": "^2.0.1",
    "@parcel/transformer-typescript-tsc": "^2.0.1",
    "@parcel/validator-eslint": "^2.0.1",
    "@parcel/validator-typescript": "^2.0.1",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "autoprefixer": "^10.4.0",
    "chai": "^4.3.6",
    "eslint": "^8.3.0",
    "eslint-config-google": "^0.14.0",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^9.2.1",
    "parcel": "^2.0.0-rc.0",
    "parcel-transformer-pug-precompile": "^1.0.6",
    "parcel-validator-stylelint": "^1.0.1",
    "postcss": "^8.0.0",
    "postcss-custom-properties": "^12.0.0",
    "postcss-import": "^14.0.2",
    "postcss-modules": "^4.2.2",
    "postcss-url": "^10.1.3",
    "sharp": "^0.29.1",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "ts-node": "^10.6.0",
    "typescript": "^4.5.3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          "> 1%",
          "last 4 versions",
          "ie >= 9"
        ]
      }
    }
  },
  "dependencies": {
    "express": "^4.17.1",
    "pug": "^3.0.2"
  }
}
